{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Config (Default)",
			"type": "shell",
			"command": "cmake",
			"args": [
				"-S", "${workspaceFolder}",
				"-B", "${workspaceFolder}/build"
			]
		},
		{
			"label": "Build (Default)",
			"type": "shell",
			"dependsOn":[
				"Config (Default)"
			],
			"dependsOrder": "sequence",
			"command": "cmake",
			"args": [
				"--build", "${workspaceFolder}/build"
			],
		}
	]
}